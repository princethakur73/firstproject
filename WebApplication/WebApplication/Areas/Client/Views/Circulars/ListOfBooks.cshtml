                                            <tr>
                                                <td>@item.Date.ToString("dd MMM, yyyy")</td>
                                                <td>@item.Title</td>
                                                <td align="center"><a target='_blank' href="/Content/files/downloads/@item.FileName"><img src="~/Content/Client/images/download.png" /></a> </td>
