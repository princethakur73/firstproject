@model IEnumerable<WebApplication.Models.MemberModel>
<!--============================
    About Page Header
=============================-->
@{
    var i = 1;
}
<style>
    table td {
        padding: 10px;
    }
</style>
<section class="page-header">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <h1>Management Members</h1>
                <ul class="list-unstyled">
                    <li><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li class="active">Management Members</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<!-- Ends: .page-header -->
<!--============================
        About Page Content
    =============================-->
<section class="all-teachers-wrapper">
    <div class="container">
        <div class="row">
            <div class="col-md-3 col-sm-12 ">
                @Html.Partial("~/Areas/Client/Views/AboutUs/_AboutUsSidebar.cshtml", 9)
            </div>
            <div class="col-sm-9">
                <p class="justify">The institution is running under Harsh Educational Society registered under the Society Registration Act 1860. Registration no. of the society is 0048.</p>
                <table class="table" border="1" cellpadding="10" style="width:100%">
                    <tr><th>S. No.</th><th>Name</th><th>Designation</th></tr>
                    @foreach (var item in Model)
                    {
                        <tr><td>@i</td><td>@item.Name</td><td>@item.Desigination</td></tr>                       
                        i++;
                    }
                </table>
            </div>
        </div>
    </div>
</section>