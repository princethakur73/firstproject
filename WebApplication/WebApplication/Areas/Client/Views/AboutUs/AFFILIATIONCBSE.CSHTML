@model WebApplication.Models.PageModel

@section seo
{
    <meta name="description" content="@Model.MetaDescription">
    <meta name="keywords" content="@Model.MetaKeywords">
}
<!--============================
        About Page Header
    =============================-->
<section class="page-header">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <h1>@Model.Title</h1>
                <ul class="list-unstyled">
                    <li><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li class="active">@Model.Title</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<!-- Ends: .page-header -->
<!--============================
        About Page Content
    =============================-->
<section class="about-ds">
    <div class="container">
        <div class="row">
            <div class="col-md-3 col-sm-12 ">
                @Html.Partial("~/Areas/Client/Views/AboutUs/_AboutUsSidebar.cshtml", 4)
            </div>
            <div class="col-sm-9">
                @Html.Raw(Model.Contents)
            </div>
            <div class="col-md-9">
                <table class="table table-hover table-bordered table-responsive">
                    <thead>
                        <tr>
                            <th>Title </th>
                            <th>Download </th>
                            <th>View</th>
                        </tr>
                    </thead>
                    <tr><td>Details Of Fee</td><td><span><a target="_self" href="~/Content/files/CBSE-Affiliation/Detail-of-fee.pdf">Download</a></span></td><td><span><a target="_blank" href="~/Content/files/CBSE-Affiliation/Detail-of-fee.pdf">View</a></span></td></tr>
                    <tr><td>Performa Form</td><td><span><a target="_self" href="~/Content/files/CBSE-Affiliation/Performa-certificate.pdf">Download</a></span></td><td><span><a target="_blank" href="~/Content/files/CBSE-Affiliation/Performa-certificate.pdf">View</a></span></td></tr>
                </table>
            </div>
        </div>
    </div>
</section>